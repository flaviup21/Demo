{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "fipa2019adf"
		},
		"ADLSDestination_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSDestination'"
		},
		"AzureDataLakeAnalytics1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureDataLakeAnalytics1'"
		},
		"BlobStorageSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageSource'"
		},
		"ADLSDestination_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakestoragefipa.dfs.core.windows.net"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "adlanalyticsfipa"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "feeb9294-2e3f-43de-bd12-e6e4301b910b"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "a136b051-8551-4674-92c1-8dc4abb2b592"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "aca480e6-8a4b-4105-91ce-3c5942657b43"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "AdfDemo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSDestination_accountKey": {
						"value": "[parameters('ADLSDestination_accountKey')]"
					},
					"AzureDataLakeAnalytics1_servicePrincipalKey": {
						"value": "[parameters('AzureDataLakeAnalytics1_servicePrincipalKey')]"
					},
					"BlobStorageSource_connectionString": {
						"value": "[parameters('BlobStorageSource_connectionString')]"
					},
					"ADLSDestination_properties_typeProperties_url": {
						"value": "[parameters('ADLSDestination_properties_typeProperties_url')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_accountName": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_accountName')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}