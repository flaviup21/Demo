{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "fipa2019adf"
		},
		"ADLSDestination_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSDestination'"
		},
		"BlobStorageSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageSource'"
		},
		"ADLSDestination_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakestoragefipa.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSDestination_accountKey": {
						"value": "[parameters('ADLSDestination_accountKey')]"
					},
					"BlobStorageSource_connectionString": {
						"value": "[parameters('BlobStorageSource_connectionString')]"
					},
					"ADLSDestination_properties_typeProperties_url": {
						"value": "[parameters('ADLSDestination_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}